{
  "name": "ignem-arcanium-server",
  "version": "1.0.0",
  "description": "Ignem Arcanium Server Project",
  "main": "dist/src/main/index.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/main/index.js",
    "predev": "npm run build",
    "dev": "node ./dist/src/main/index.js",
    "build": "rimraf dist && tsc -p ./tsconfig-build.json",
    "build:watch": "tsc -p ./tsconfig-build.json --watch",
    "test": "jest --runInBand --no-cache --forceExit",
    "test:cov": "docker-compose up -d pg-tests && npm test -- --coverage",
    "test:unit": "npm test -- --watch --roots ./tests/unit --roots ./src",
    "test:int": "docker-compose up -d pg-tests && npm test -- --watch --roots ./tests/integration --roots ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabriel-JM/ignem-arcanium-server.git"
  },
  "author": "Gabriel Jos√©",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gabriel-JM/ignem-arcanium-server/issues"
  },
  "homepage": "https://github.com/Gabriel-JM/ignem-arcanium-server#readme",
  "_moduleAliases": {
    "@": "dist/src"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/websocket": "^1.0.5",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "jest": "^27.4.4",
    "rimraf": "^3.0.2",
    "superwstest": "^2.0.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv-safe": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.2",
    "module-alias": "^2.2.2",
    "nanoid": "^3.1.30",
    "pg": "^8.7.3",
    "websocket": "^1.0.34"
  }
}

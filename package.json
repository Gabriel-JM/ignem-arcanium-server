{
  "name": "ignem-arcanium-server",
  "version": "1.0.0",
  "description": "Ignem Arcanium Server Project",
  "main": "dist/src/main/index.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/main/index.js",
    "predev": "npm run build",
    "dev": "npm start",
    "build": "rimraf dist && tsc -p ./tsconfig-build.json",
    "build:watch": "tsc -p ./tsconfig-build.json --watch",
    "test": "vitest",
    "test:unit": "npm test -- -r='./tests/unit'",
    "test:int": "docker-compose -f docker-compose.test.yml up -d && sleep 10s && npm test -- -r='./tests/integration'",
    "posttest:int": "docker-compose -f docker-compose.test.yml down",
    "test:cov": "npm run test:unit -- --run --coverage",
    "check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabriel-JM/ignem-arcanium-server.git"
  },
  "author": "Gabriel Jos√©",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gabriel-JM/ignem-arcanium-server/issues"
  },
  "homepage": "https://github.com/Gabriel-JM/ignem-arcanium-server#readme",
  "_moduleAliases": {
    "@": "dist/src"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/websocket": "^1.0.5",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.2.2",
    "npm-check-updates": "^16.0.5",
    "rimraf": "^3.0.2",
    "superwstest": "^2.0.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv-safe": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "module-alias": "^2.2.2",
    "nanoid": "3.3.4",
    "pg": "^8.8.0",
    "websocket": "^1.0.34"
  }
}
